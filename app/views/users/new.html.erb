<p> Already have an account? <%= link_to  "Sing In", new_session_path %></p>

<% if flash[:errors] %>
      <div style="color: red;">
      <% flash[:errors].each do |message| %>
          <p><%= message %></p>
      <% end %>
      </div>
      <% end %>

<h1>SIGN UP</h1>
  
<%= form_for @user do |f| %>
<%=f.label 'First Name'%><br>
<%= f.text_field :first_name%><br><br>
<%=f.label 'Last Name'%><br>
<%= f.text_field :last_name%><br><br>
<%=f.label 'Username'%><br>
<%= f.text_field :username%><br><br>
<%=f.label 'Password'%><br>
<%= f.password_field :password_digest%><br><br>
<%=f.label 'Confirm Password'%><br>
<%= f.password_field :password%><br><br><br>

<%= f.submit 'Create'%>

<% end %><br>

